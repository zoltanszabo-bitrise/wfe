    "shell-loader": "^1.2.1",
    "start-server-and-test": "^2.0.3",
    "storybook": "^8.1.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.4",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "whatwg-fetch": "^3.6.20"
  }
}
